<style>
  .detail-container h2 {
    color: #333;
    font-weight: 500;
  }
  .detail-container p {
    color: #555;
    font-weight: 400;
  }
  .detail-container h3 {
    padding: 0;
    padding-left: 10px;
    font-size: 20px;
  }
  .detail-container h3 a {
    font-size: 14px;
    font-weight: normal;
  }
  .detail-container .red {
    border-left: 4px solid #fdae52;
  }
  .detail-container .orange {
    border-left: 4px solid #fd7f64;
  }
  .detail-container .blue {
    border-left: 4px solid #59acff;
  }
  .detail-container i {
    font-size: 14px;
    font-weight: 500;

  }
</style>
<div class="detail-container xsd-drawer house-room" id="financeDetail">
  <div class="xsd-drawer-header">
    <a class="xsd-modal-close iconfont">&#xe633;</a>
  </div>
  <div class="xsd-drawer-body">
    <div class="xsd-tabs">
      <div class="xsd-tab-content">
        <div id="tab-1" class="active xsd-tab-pane">
          <h3 class="xsd-margin-b24 blue">交易详情
          </h3>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">交易编号</span>
            <b class="xsd-pull-left">{{model.payment.transaction_id}}</b>
          </div>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">{{#if (eq model.payment.trade_object '租客')}}交易房间{{else}}交易房源{{/if}}</span>
            <b class="xsd-pull-left">{{model.payment.trade_info}}</b>
          </div>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">交易对象</span>
            <b class="xsd-pull-left">{{model.payment.trade_object}}</b>
          </div>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">交易对象姓名</span>
            <b class="xsd-pull-left">{{model.payment.trader_name}}</b>
          </div>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">交易日期</span>
            <b class="xsd-pull-left">{{model.payment.trade_at}}</b>
          </div>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">资金流向</span>
            <b class="xsd-pull-left">{{model.payment.flow_type}}</b>
          </div>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">交易方式</span>
            <b class="xsd-pull-left">{{model.payment.pay_method}}</b>
          </div>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">交易流水号</span>
            <b class="xsd-pull-left">
              {{#if model.payment.trade_serial_no}}
                {{model.payment.trade_serial_no}}
              {{else}}
                --
              {{/if}}
            </b>
          </div>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">交易金额</span>
            <b class="xsd-pull-left"><b class="xsd-color-primary">{{model.payment.amount}}</b> 元</b>
          </div>
          <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
            <span class="xsd-pull-left">其中：</span>
          </div>
{{#each model.moneyflows as | list |}}
  {{#if (eq list.fee_type 0)}}
  <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
    <span class="xsd-pull-left">租金</span>
    <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
  </div>
  {{/if}}
  {{#if (eq list.fee_type 1)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">押金</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 2)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">水费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 3)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">电费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 4)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">燃气费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 5)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">物业费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 6)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">服务费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 7)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">维修费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 8)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">保洁费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 9)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">材料费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 10)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">有线电视费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 11)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">宽带费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 12)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">卫生费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 13)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">其他费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
  {{#if (eq list.fee_type 16)}}
    <div class="xsd-input-group xsd-clearfix xsd-margin-b12">
      <span class="xsd-pull-left">优惠费</span>
      <b class="xsd-pull-left"><b class="xsd-color-primary">{{list.real_amount}}</b> 元</b>
    </div>
  {{/if}}
{{/each}}


        </div>
      </div>
    </div>

  </div>
  <div class="xsd-drawer-bottom">
  </div>
</div>

